	"license": "GPL-3.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/hanneskuettner/directus-extension-inline-form-interface.git"
	},
	"bugs": {
		"url": "https://github.com/hanneskuettner/directus-extension-inline-form-interface/issues"
